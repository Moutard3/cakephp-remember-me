{
  "name": "nojimage/cakephp-remember-me",
  "description": "RememberMe authentication adapter plugin for CakePHP 4",
  "type": "cakephp-plugin",
  "license": ["MIT"],
  "require": {
    "php": ">=7.1",
    "ext-json": "*",
    "cakephp/cakephp": "^4.0",
    "cakephp/authentication": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "RememberMe\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RememberMe\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  }
}
